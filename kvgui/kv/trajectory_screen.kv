<TrajectoryScreen>

    MDTabs:
        elevation: 2

        Tab:
            id: fast_tab
            title: "Table"
            content_text: f"This is an example text for {self.title}"

            MDScrollView:
                MDBoxLayout:
                    orientation: 'vertical'
                    size_hint_y: None
                    height: self.minimum_height
                    padding: "20dp"
                    spacing: "15dp"

                    MarkupTable:
                        id: markup_table
                        size_hint: 1, None
                        height: self.texture_size[1]
                        theme_text_color: 'Secondary'

        Tab:
            id: graph_tab
            title: "Graph"
            content_text: f"This is an example text for {self.title}"

