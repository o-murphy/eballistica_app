<TrajectoryScreen>

    MDTabs:
        elevation: 2

        Tab:
            id: table_tab
            title: "Table"
            content_text: f"This is an example text for {self.title}"

            MDLabel:
                id: not_imp
                text: "Not implemented yet"
                pos_hint: {"center_x": 0.5, 'top': 1}
                halign: 'center'

            MDProgressBar:
                id: progress
                value: 0
                pos_hint: {"center_x": 0.5, 'top': 1}
                color: 'orange'
                size_hint_y: None

        Tab:
            id: graph_tab
            title: "Graph"
            content_text: f"This is an example text for {self.title}"

            MDLabel:
                id: not_imp
                text: "Not implemented yet"
                pos_hint: {"center_x": 0.5, 'top': 1}
                halign: 'center'
    